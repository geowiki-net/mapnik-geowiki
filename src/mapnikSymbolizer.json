{
  "PolygonSymbolizer": {
    "filterField": "fill",
    "fieldMapping": {
      "fill": "fillColor",
      "fill-opacity": "fillOpacity",
      "gamma": "gamma",
      "smooth": "smoothFactor",
      "comp-op": "compositingOperation"
    }
  },
  "LineSymbolizer": {
    "filterField": "stroke",
    "fieldMapping": {
      "stroke": "color",
      "stroke-width": "width",
      "stroke-opacity": "opacity",
      "stroke-linejoin": "lineJoin",
      "stroke-linecap": "lineCap",
      "stroke-dasharray": "dashArray",
      "offset": "offset",
      "comp-op": "compositingOperation",
      "smooth": "smoothFactor"
    }
  },
  "TextSymbolizer": {
    "requireField": "text",
    "content": "[text]",
    "fieldMapping": {
      "face-name": "textFontName",
      "size": "textFontSize",
      "dx": "textMoveX",
      "dy": "textMoveY",
      "placement": "textPlacement",
      "opacity": "textOpacity",
      "fill": "textColor",
      "horizontal-alignment": "textHorizontalAlign",
      "vertical-alignment": "textVerticalAlign",
      "halo-fill": "textHaloColor",
      "halo-radius": "textHaloRadius",
      "halo-comp-op": "textHaloCompositingOperation",
      "character-spacing": "textLetterSpacing"
    }
  }
}
